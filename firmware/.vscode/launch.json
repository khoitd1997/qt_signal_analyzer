{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceRoot}",
      "executable": "./build/signal_analyzer_firmware.elf",
      "name": "Debug STM32407VG",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "device": "STM32F407VG",
      "svdFile": "STM32F407.svd",
      "preLaunchTask": "build+run",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 168000000,
        "decoders": [
          {
            "showOnStartup": true,
            "type": "console",
            "port": 0
          }
        ],
      }
    }
  ]
}